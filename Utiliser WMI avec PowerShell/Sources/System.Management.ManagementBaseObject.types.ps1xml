<?xml version="1.0" encoding="utf-8" ?>
<!-- *******************************************************************
Ajoute les scriptmethod ConvertToDateTime et ConvertFromDateTime aux instances de la classe ManagementBaseObject
PS v1 ne gére que les instance WMI ManagementObject
******************************************************************** -->
<Types>
 <Type>
  <Name>System.Management.ManagementBaseObject</Name>
  <Members>
      <ScriptMethod>
          <Name>ConvertToDateTime</Name>
          <Script>
              [System.Management.ManagementDateTimeConverter]::ToDateTime($args[0])
          </Script>
      </ScriptMethod>
      <ScriptMethod>
          <Name>ConvertFromDateTime</Name>
          <Script>
              [System.Management.ManagementDateTimeConverter]::ToDmtfDateTime($args[0])
          </Script>
      </ScriptMethod>
      <ScriptMethod>
          <Name>Put</Name>
          <Script>
            $this.PSBase.Put()
          </Script>
      </ScriptMethod>
      <ScriptMethod>
        <Name>Delete</Name>
        <Script>
          $this.PSBase.Delete()
        </Script>
      </ScriptMethod>
      <ScriptMethod>
        <Name>GetType</Name>
        <Script>
          $this.PSBase.GetType()
        </Script>
      </ScriptMethod>         
  </Members>
 </Type>
</Types>
